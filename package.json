    "concurrently": "^4.1.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.6.4",
    "multer": "^1.4.2",
    "nodemailer": "^4.6.7",
    "path": "^0.12.7",
    "validator": "^11.1.0"
